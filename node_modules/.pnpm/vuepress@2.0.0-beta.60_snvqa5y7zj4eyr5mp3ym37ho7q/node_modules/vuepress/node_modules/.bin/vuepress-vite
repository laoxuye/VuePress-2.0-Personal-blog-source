#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/project/Vue/vuePressBlogTwo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/project/Vue/vuePressBlogTwo/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vuepress-vite@2.0.0-beta.60_snvqa5y7zj4eyr5mp3ym37ho7q/node_modules/vuepress-vite/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../../../../vuepress-vite@2.0.0-beta.60_snvqa5y7zj4eyr5mp3ym37ho7q/node_modules/vuepress-vite/bin/vuepress.js" "$@"
fi
